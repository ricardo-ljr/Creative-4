;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,QAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA,CAAA,CAAA,CAAA;AACA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA,CAAA,MAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,KATA;AAUA,IAAA,WAAA,EAAA,uBAAA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,MAAA,EAAA,KAAA,QAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,KAAA,GAAA,CAAA;AACA,KAdA;AAeA,IAAA,uBAAA,EAAA,mCAAA;AACA,WAAA,MAAA,CAAA,IAAA,CAAA,MAAA,EAAA,IAAA;AACA,WAAA,KAAA,GAAA,CAAA;AACA;AAlBA,GAVA;AA8BA,EAAA,OA9BA,qBA8BA;AACA,SAAA,MAAA,CAAA,EAAA,CACA,SADA,EAEA,UAAA,OAAA,EAAA;AACA,WAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,YAAA,UAAA,GAAA,QAAA,CAAA,cAAA,CAAA,SAAA,CAAA;AACA,QAAA,UAAA,CAAA,SAAA,GAAA,UAAA,CAAA,YAAA;AACA,OAHA;AAIA,KANA,CAMA,IANA,CAMA,IANA,CAFA;AAUA;AAzCA,CAAA","sourcesContent":["<template>\n  <div>\n    <h1>Gym Chat</h1>\n    <div id=\"app\">\n      <div v-if=\"state == 0\">\n        <h3>Welcome! Please put your name in</h3>\n        <form @submit.prevent=\"setUsername\">\n          <input type=\"text\" placeholder=\"Username...\" v-model=\"username\" />\n          <input type=\"submit\" value=\"Join\" />\n        </form>\n        <button @click=\"continueWithoutUsername\">Join as a Guest</button>\n      </div>\n      <div v-if=\"state == 1\">\n        <ul id=\"chatbox\">\n          <li v-for=\"message in messages\" v-bind:key=\"message\">\n            <b>{{ message.user }}:</b> {{ message.message }}\n          </li>\n        </ul>\n        <form @submit.prevent=\"sendMessage\">\n          <input type=\"text\" placeholder=\"Message...\" v-model=\"message\" />\n          <input type=\"submit\" value=\"Send\" />\n        </form>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { io } from \"socket.io-client\";\n\nexport default {\n  data() {\n    return {\n      messages: [],\n      message: \"\",\n      username: \"\",\n      state: 0,\n      socket: io()\n    };\n  },\n  methods: {\n    sendMessage: function() {\n      this.socket.emit(\"message\", this.message);\n      for (let i = 0; i < this.messages.length; i++) {\n        console.log(this.messages[i]);\n      }\n      console.log(this.messages.length);\n      console.log(this.username);\n      this.message = \"\";\n    },\n    setUsername: function() {\n      this.socket.emit(\"join\", this.username);\n      this.username = \"\";\n      this.state = 1;\n    },\n    continueWithoutUsername: function() {\n      this.socket.emit(\"join\", null);\n      this.state = 1;\n    }\n  },\n  created() {\n    this.socket.on(\n      \"message\",\n      function(message) {\n        this.messages.push(message);\n        this.$nextTick(function() {\n          var messageBox = document.getElementById(\"chatbox\");\n          messageBox.scrollTop = messageBox.scrollHeight;\n        });\n      }.bind(this)\n    );\n  }\n};\n</script>\n\n<style scoped>\n#chatbox {\n  height: 200px;\n  max-width: 400px;\n  overflow-y: scroll;\n}\n</style>\n"],"sourceRoot":"src/components"}]}