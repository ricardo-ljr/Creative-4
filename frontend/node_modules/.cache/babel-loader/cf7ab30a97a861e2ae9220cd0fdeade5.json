A,GAAA,KAAA;AACA,KANA;AAOA,IAAA,UAAA,EAAA,oBAAA,OAAA,EAAA;AACA,WAAA,OAAA,CAAA,EAAA,CAAA;AACA,QAAA,IAAA,EAAA;AADA,OAAA;AAGA,KAXA;AAYA,IAAA,WAZA,yBAYA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,cAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,GAAA;AACA,gBAAA,KAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;AAJA,iDAKA,IALA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KArBA;AAsBA,IAAA,WAtBA,uBAsBA,OAtBA,EAsBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAGA,KAAA,CAAA,GAAA,CAAA,kBAAA,OAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,WAAA,EAAA,MAAA,CAAA,WAFA;AAGA,kBAAA,IAAA,EAAA,MAAA,CAAA,IAHA;AAIA,kBAAA,WAAA,EAAA,MAAA,CAAA;AAJA,iBAAA,CAHA;;AAAA;AAGA,gBAAA,MAHA;AASA,gBAAA,MAAA,CAAA,QAAA,GAAA,MAAA,CAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,WAAA;;AAVA;AAAA;;AAAA;AAAA;AAAA;AAYA,gBAAA,OAAA,CAAA,GAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KApCA;AAqCA,IAAA,aArCA,yBAqCA,OArCA,EAqCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,kBAAA,OAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,WAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,WAAA;;AAJA,kDAKA,IALA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA;AA9CA;AAbA,CAAA","sourcesContent":["<template>\n  <div class=\"home\">\n    <section class=\"add-button\">\n      <router-link to=\"/add-workout\" tag=\"button\">Add Workout</router-link>\n    </section>\n    <section class=\"workouts\">\n      <div class=\"workout-card\" v-for=\"workout in workouts\" :key=\"workout.id\">\n        <h2>{{ workout.title }}</h2>\n        <p>Instructions: {{ workout.description }}</p>\n        <p>Sets: {{ workout.sets }}</p>\n        <p>Repetitions: {{ workout.repetitions }}</p>\n        <button @click=\"deleteWorkout(workout)\">Completed</button>\n        <button @click=\"showForm()\">Edit Workout</button>\n        <my-modal v-show=\"modal\">\n          <div>\n            <div class=\"form\">\n              <input v-model=\"title\" placeholder=\"Title...\" />\n              <input v-model=\"description\" placeholder=\"Description...\" />\n              <input v-model=\"sets\" placeholder=\"Sets...\" />\n              <input v-model=\"repetitions\" placeholder=\"Repetions...\" />\n              <p></p>\n              <button\n                @click=\"\n                  editWorkout(workout);\n                  hideForm();\n                \"\n              >Upload</button>\n            </div>\n          </div>\n        </my-modal>\n      </div>\n      <div>\n        <a href=\"https://github.com/ricardo-ljr/Creative-4\" target=\"_blank\">Github</a>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\naxios.defaults.baseURL = \"http://localhost:3000\";\n\nexport default {\n  name: \"Home\",\n\n  data() {\n    return {\n      modal: false,\n      workouts: [],\n      findWorkout: null\n    };\n  },\n  created() {\n    this.getWorkouts();\n  },\n  methods: {\n    showForm() {\n      this.modal = true;\n    },\n    hideForm() {\n      this.modal = false;\n    },\n    navigateTo: function(workout) {\n      this.$router.go({\n        path: workout\n      });\n    },\n    async getWorkouts() {\n      try {\n        let response = await axios.get(\"/api/workout\");\n        console.log(response._id);\n        this.workouts = response.data;\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async editWorkout(workout) {\n      try {\n        // const formData = new FormData();\n        let update = await axios.put(\"/api/workout/\" + workout._id, {\n          title: this.title,\n          description: this.description,\n          sets: this.sets,\n          repetitions: this.repetitions\n        });\n        this.editItem = update.data;\n        this.getWorkouts();\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async deleteWorkout(workout) {\n      try {\n        await axios.delete(\"/api/workout/\" + workout._id);\n        this.findWorkout = null;\n        this.getWorkouts();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.workouts {\n  padding: 2px 16px;\n}\n.workout-card {\n  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);\n  transition: 0.3s;\n}\n.workout-card:hover {\n  box-shadow: 0 8px 16px 0 rgba(0, 0, 0, 0.2);\n}\n\ninput,\ntextarea,\nselect {\n  font-size: 18px;\n  display: block;\n  margin-left: auto;\n  margin-right: auto;\n  background: transparent;\n  border: none;\n  border-bottom: 1px dashed #83a4c5;\n  width: 275px;\n  text-align: center;\n  outline: none;\n  padding: 15px 5px 15px 15px;\n  font-style: italic;\n}\n\n.form {\n  margin: 0 auto;\n  width: 550px;\n  font-size: 16px;\n  padding: 30px 30px 15px 30px;\n}\n\nbutton {\n  margin: 5px;\n}\n</style>\n"],"sourceRoot":"src/views"}]}