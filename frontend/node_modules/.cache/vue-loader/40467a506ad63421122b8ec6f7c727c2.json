input\n            id=\"nickname\"\n            :state=\"state\"\n            v-model.trim=\"chat.nickname\"\n          ></b-form-input>\n        </b-form-group>\n        <b-button type=\"submit\" variant=\"primary\">Join</b-button>\n      </b-form>\n    </b-col>\n  </b-row>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"JoinRoom\",\n  data() {\n    return {\n      chat: {}\n    };\n  },\n  methods: {\n    onSubmit(evt) {\n      evt.preventDefault();\n      this.chat.room = this.$route.params.id;\n      this.chat.message = this.chat.nickname + \" join the room\";\n      axios\n        .post(`http://localhost:3000/api/chat`, this.chat)\n        .then(response => {\n          this.$router.push({\n            name: \"ChatRoom\",\n            params: {\n              id: this.$route.params.id,\n              nickname: response.data.nickname\n            }\n          });\n        })\n        .catch(e => {\n          this.errors.push(e);\n        });\n    }\n  }\n};\n</script>\n"]}]}