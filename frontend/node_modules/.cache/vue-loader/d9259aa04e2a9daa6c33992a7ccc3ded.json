\n\n<script>\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      title: \"\",\n      description: \"\",\n      sets: null,\n      repetitions: null,\n      addItem: null\n    };\n  },\n  methods: {\n    async addWorkout() {\n      try {\n        // const formData = new FormData();\n        let workout = await axios.post(\"/api/workout\", {\n          title: this.title,\n          description: this.description,\n          sets: this.sets,\n          repetitions: this.repetitions\n        });\n        this.addItem = workout.data;\n        this.$router.push(\"/\");\n      } catch (error) {\n        console.log(error);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\ninput,\ntextarea,\nselect {\n  font-size: 18px;\n  display: block;\n  margin-left: auto;\n  margin-right: auto;\n  background: transparent;\n  border: none;\n  border-bottom: 1px dashed #83a4c5;\n  width: 275px;\n  text-align: center;\n  outline: none;\n  padding: 15px 5px 15px 15px;\n  font-style: italic;\n}\n\n.form {\n  margin: 0 auto;\n  width: 550px;\n  font-size: 16px;\n  padding: 30px 30px 15px 30px;\n}\n</style>\n"]}]}